srcdir=         @srcdir@
top_srcdir=     @top_srcdir@
top_builddir=   @top_builddir@
VPATH=          @srcdir@

include ${top_builddir}/Mk/autoconf.mk
include ${top_builddir}/Mk/compile.mk
include ${top_builddir}/Mk/subdir.mk

OBJS=	TRArrayTests.o \
	TRAutoreleasePoolTests.o \
	TRHashTests.o \
	TRStringTests.o
LIB=	libauth-ldap-test-util.a

CFLAGS+=	@CHECK_CFLAGS@
OBJCFLAGS+=	@CHECK_CFLAGS@

all:: $(LIB)

$(LIB): $(OBJS)
	$(AR) -r $@ $(OBJS)

clean::
	rm -f $(OBJS) $(LIB)

distclean:: clean
	rm -f Makefile
