srcdir=         @srcdir@
top_srcdir=     @top_srcdir@
top_builddir=   @top_builddir@
VPATH=          @srcdir@

include ${top_builddir}/Mk/autoconf.mk
include ${top_builddir}/Mk/compile.mk
include ${top_builddir}/Mk/subdir.mk

OBJS=	mockpf.o \
	TRPacketFilter.o \
	TRPFAddress.o
LIB=	libauth-ldap-test-pf.a

CFLAGS+=	@CHECK_CFLAGS@
OBJCFLAGS+=	@CHECK_CFLAGS@

all:: $(LIB)

$(LIB): $(OBJS)
	$(AR) -r $@ $(OBJS)
	# XXX remove once the library isn't empty on non-FreeBSD platforms
	$(RANLIB) $@

clean::
	rm -f $(OBJS) $(LIB)

distclean:: clean
	rm -f Makefile
