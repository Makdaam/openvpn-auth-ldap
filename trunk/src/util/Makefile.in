srcdir=         @srcdir@
top_srcdir=     @top_srcdir@
top_builddir=   @top_builddir@
VPATH=          @srcdir@

include ${top_builddir}/Mk/autoconf.mk
include ${top_builddir}/Mk/compile.mk
include ${top_builddir}/Mk/subdir.mk

LIB_OBJS=	hash.o \
		TRString.o \
		TRArray.o \
		TREnumerator.o \
		TRHash.o \
		strlcpy.o \
		xmalloc.o

LIB=		libauth-ldap-util.a

all:: $(LIB)

$(LIB): $(LIB_OBJS)
	$(AR) -r $@ $(LIB_OBJS)

clean::
	rm -f $(LIB_OBJS) $(LIB)

distclean:: clean
	rm -f Makefile
